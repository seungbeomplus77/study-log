        http://www.springframework.org/schema/task http://www.springframework.org/schema/task/spring-task.xsd">
		
		<context:annotation-config/> <!-- annotation 활성화 -->
		
		<bean id="userService" class="com.anno.user4.UserServiceImpl"/>
		<bean id="userService2" class="com.anno.user4.UserServiceImpl2"/>
		
		<bean id="user" class="com.anno.user4.User"/>
		<bean id="user2" class="com.anno.user4.User2"/>

